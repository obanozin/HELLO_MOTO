
<table>
	<thead>
		<tr>
			<th>ID</th>
			<th>名前</th>
			<th>リーダー名</th>
			<th>削除</th>
		</tr>
	</thead>

	<tbody>
		<% @teams.each do |team| %>
			<tr>
				<td>
					<%= team.id %>
				</td>
				<td>
					<%= team.team_name %>
				</td>
				<td>
					<%= team.user.nickname %>
				</td>
				<td>
					<%= link_to "delete", team_path(team.id), method: :delete %>
				</td>
			</tr>
		<% end %>
		<%= paginate @teams %>
	</tbody>
	<tr>
		<th><h3><%= link_to "戻る", admins_path %></h3></th>
	</tr>
</table>